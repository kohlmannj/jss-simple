{
  "name": "jss-simple",
  "description": "Thin wrapper around jss that simplifies its interface and restricts you to a single stylesheet.",
  "repository": "git://github.com/ashaffer/jss-simple.git",
  "version": "0.4.0-beta.0",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "private": true,
  "scripts": {
    "build": "babel src --out-dir lib",
    "lint-script": "eslint --fix {src,__test__}/**/*.js",
    "postpublish": "rimraf lib",
    "precommit": "lint-staged",
    "prepublish": "npm run test && npm run build",
    "test": "jest --coverage"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "lib"
    ]
  },
  "dependencies": {
    "jss": "^7.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-preset-latest": "^6.24.1",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^1.7.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-jsx-a11y": "^3.0.2 || ^4.0.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "lint-staged": "^3.4.1",
    "np": "^2.14.1",
    "prettier": "^1.3.1",
    "rimraf": "^2.6.1",
    "standard": "^5.4.1"
  }
}
